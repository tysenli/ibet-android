<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <Button
            android:text=""
            android:layout_width="136dp"
            android:layout_height="26dp"
            android:background="@drawable/depo_select"
            android:id="@+id/deposit" android:layout_marginTop="20dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="48dp" app:layout_constraintStart_toStartOf="parent"
    />
    <Button
            android:text=""
            android:layout_width="136dp"
            android:layout_height="26dp"
            android:background="@drawable/withdraw_btn"
            android:id="@+id/withdraw"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="48dp" android:layout_marginTop="20dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/deposit" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.645"/>
    <Button android:id="@+id/qai_wechat"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="QAIwechat"
            android:textSize="12sp"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView33" android:layout_marginStart="24dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button android:id="@+id/qai_bt"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="QAI BT"
            android:textSize="12sp"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView33" app:layout_constraintStart_toEndOf="@+id/qai_ali"
            android:layout_marginStart="36dp"/>
    <Button android:id="@+id/asia_wechat"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="Wechatpay"
            android:textSize="12sp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/qai_wechat" android:layout_marginStart="24dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button android:id="@+id/asia_alipay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="Alipay"
            android:textSize="12sp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/qai_ali" app:layout_constraintStart_toEndOf="@+id/asia_wechat"
            android:layout_marginStart="20dp"/>
    <Button android:id="@+id/qai_union"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="QAI UNION"
            android:textSize="12sp"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView33" app:layout_constraintStart_toEndOf="@+id/qai_bt"
            android:layout_marginStart="24dp"/>
    <TextView
            android:layout_width="222dp"
            android:layout_height="25dp"
            android:textSize="15sp"
            android:textColor="#212121"
            android:text="Add Payment method"
            android:id="@+id/textView33"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.092" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/deposit"/>
    <View
            android:id="@+id/divider"
            android:layout_width="360dp"
            android:layout_height="1dp"
            android:background="#e4e4e4"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView33" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
    />
    <Button android:id="@+id/paypal"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/paypal"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/astropay" app:layout_constraintStart_toEndOf="@+id/ciclepay"
            android:layout_marginStart="36dp"/>

    <Button android:id="@+id/line_pay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/linepay"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/scratch" app:layout_constraintStart_toEndOf="@+id/paypal"
            android:layout_marginStart="24dp"/>
    <Button android:id="@+id/asia_bank"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/bank"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/qai_bt" app:layout_constraintStart_toEndOf="@+id/asia_alipay"
            android:layout_marginStart="36dp"/>

    <Button android:id="@+id/qai_ali"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="QAIalipay"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView33" app:layout_constraintStart_toEndOf="@+id/qai_wechat"
            android:layout_marginStart="20dp"/>

    <Button android:id="@+id/asia_unionpay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/unionpay"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/qai_union" app:layout_constraintStart_toEndOf="@+id/asia_bank"
            android:layout_marginStart="24dp"/>

    <Button android:id="@+id/asia_jdpay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/jdpay"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/asia_bank" app:layout_constraintStart_toEndOf="@+id/asia_quickpay"
            android:layout_marginStart="36dp"/>

    <Button android:id="@+id/asia_quickpay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/quickpay"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/asia_alipay" app:layout_constraintStart_toEndOf="@+id/bitcoin"
            android:layout_marginStart="20dp"/>

    <Button android:id="@+id/bitcoin"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/bitcoin"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/asia_wechat" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"/>

    <Button android:id="@+id/visa"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:background="@drawable/cards"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/asia_unionpay"
            app:layout_constraintStart_toEndOf="@+id/asia_jdpay" android:layout_marginStart="24dp"/>
    <Button android:id="@+id/help2pay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="help2pay"
            android:textSize="12sp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/bitcoin" android:layout_marginStart="24dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button android:id="@+id/fgate"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="fgate"
            android:textSize="12sp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/help2pay" android:layout_marginStart="24dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button android:id="@+id/ciclepay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="ciclepay"
            android:textSize="12sp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/payzod" app:layout_constraintStart_toEndOf="@+id/fgate"
            android:layout_marginStart="20dp"/>
    <Button android:id="@+id/scratch"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="scratch"
            android:textSize="12sp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/visa" app:layout_constraintStart_toEndOf="@+id/astropay"
            android:layout_marginStart="24dp"/>
    <Button android:id="@+id/payzod"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="payzod"
            android:textSize="12sp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/asia_quickpay" app:layout_constraintStart_toEndOf="@+id/help2pay"
            android:layout_marginStart="20dp"/>
    <Button android:id="@+id/astropay"
            android:layout_width="70dp"
            android:layout_height="50dp"
            android:text="astropay"
            android:textSize="12sp"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/asia_jdpay" app:layout_constraintStart_toEndOf="@+id/payzod"
            android:layout_marginStart="36dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>